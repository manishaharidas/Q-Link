<!DOCTYPE html>
<html>

  <link href="/assets/profile.css.scss" media="screen" rel="stylesheet" type="text/css" />
 
<div id="profile_form">

<h2 id="heading">Profile Updation</h2>
<div id="form_fields">
 <%=  form_for :profile, :url => {:controller => 'profiles', :action => 'create'} do |f| %>
   <%= f.label :first_name %><%= f.text_field :first_name, :autofocus => true %></br>

   <%= f.label :last_name %><%= f.text_field :last_name , :autofocus => true%></br>

   <%= f.label :date_of_birth %><%= f.text_field :date_of_birth, 'data-behaviour' => 'datepicker' %></br>

   <%= f.label :place %><%= f.text_field :place, :autofocus => true %></br>


   <div id="submit"><%= f.submit "Update" %></div>
</div>
  <% end %>

</div>
</html>